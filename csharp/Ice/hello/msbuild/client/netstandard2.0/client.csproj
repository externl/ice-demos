    </PropertyGroup>
    <ItemGroup>
        <Compile Include="../../../Client.cs" />
        <SliceCompile Include="../../../Hello.ice" />
        <PackageReference Include="zeroc.ice.net" Version="3.7.1" />
        <PackageReference Include="zeroc.icebuilder.msbuild" Version="5.0.4" />
